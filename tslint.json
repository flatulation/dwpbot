{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "no-shadowed-variable": false,
        "ordered-imports": false,
        "no-console": false,
        "member-access": [true, "no-public"],
        "member-ordering": false,
        "curly": [true, "ignore-same-line"],
        "max-line-length": [true, 80]
    },
    "rulesDirectory": []
}